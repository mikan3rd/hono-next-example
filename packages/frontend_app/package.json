{
  "name": "frontend_app",
  "version": "0.1.0",
  "private": true,
  "imports": {
    "#src/supabase/client": {
      "storybook": "./src/supabase/client/index.mock.ts",
      "default": "./src/supabase/client/index.ts"
    }
  },
  "scripts": {
    "dev": "bun run --bun next dev",
    "next:build": "bun run --bun next build",
    "next:start": "bun run --bun next start",
    "typecheck": "bun run --bun tsgo --noEmit",
    "lint": "bun run --bun biome check . --error-on-warnings",
    "lint:fix": "bun run --bun biome check . --write --unsafe",
    "dev-storybook": "bun run --bun storybook dev -p 6006",
    "build-storybook": "bun run storybook build",
    "test-storybook": "bun run vitest --project=storybook",
    "gen:client": "bun run orval",
    "check:all": "bun install && bun run gen:client && bun run lint:fix && bun run typecheck && bun run test-storybook --run"
  },
  "dependencies": {
    "@js-temporal/polyfill": "0.5.1",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-slot": "1.2.4",
    "@supabase/ssr": "0.8.0",
    "@supabase/supabase-js": "2.87.1",
    "@t3-oss/env-nextjs": "0.13.10",
    "@tailwindcss/postcss": "4.1.18",
    "@tanstack/react-query": "5.90.12",
    "@types/node": "24.10.4",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.561.0",
    "next": "16.0.10",
    "next-themes": "0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sonner": "2.0.7",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "1.4.0",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@chromatic-com/storybook": "4.1.3",
    "@faker-js/faker": "10.1.0",
    "@storybook/addon-docs": "10.1.10",
    "@storybook/addon-vitest": "10.1.10",
    "@storybook/nextjs-vite": "10.1.10",
    "@typescript/native-preview": "7.0.0-dev.20251220.1",
    "@vitest/browser": "4.0.16",
    "@vitest/browser-playwright": "4.0.16",
    "@vitest/coverage-v8": "4.0.16",
    "msw": "2.12.4",
    "msw-storybook-addon": "2.0.6",
    "orval": "7.17.2",
    "playwright": "1.57.0",
    "storybook": "10.1.10",
    "vitest": "4.0.16"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
